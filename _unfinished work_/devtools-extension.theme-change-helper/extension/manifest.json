{
	"manifest_version": 2,

	"name": "DevTools HTML snapshot",
	"description": "open snapshot of Dev Tools html in new Tab",
	"version": "1.0",

	"minimum_chrome_version": "10.0",
	"devtools_page": "devtools.html",

	"browser_action": {
		"default_icon": "dom.png",
		"default_popup": "popup.html",
		"default_title": "Click here"
	},
	"permissions": [
		"activeTab"
	]
}