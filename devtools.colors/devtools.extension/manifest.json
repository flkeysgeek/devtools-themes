{
	//"content_security_policy": "script-src 'self'; object-src 'self'",
	
	"name": "devtools colors",
	"description": "Change Developer Tools Colors",

	"icons": {
	  "16": "colors-16.png",
	  "48": "colors-48.png",
	  "128": "colors-128.png"
	},
    "browser_action": {
        "default_icon": "colors-16.png",
        "default_popup": "color_picker.html"
	},
    "devtools_page": "devtools.colors.html",
	"background": {
		"scripts": ["background.js"]
	},   
	"options_page": "options.html",	
	
	//done by default
	//<link href="./colors-16.png" rel="panel icon" />

	"web_accessible_resources": [
		"./*.png"
	],	
    
    "key": "",
	"manifest_version": 2,
	"minimum_chrome_version": "59",
	
    "permissions": [
    	"tabs", "contextMenus", "debugger"
    	/*
    	, 
    	{
    		"fileSystem": ["write", "retainEntries", "directory"]
    	}
    	*/
    ],
	"optional_permissions": [ "debugger" ],
	
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "1.0.1"
}
